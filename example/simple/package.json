{
  "name": "simple",
  "version": "1.0.0",
  "description": "Example setup for various Rollup module(es/umd/cjs) type with babel(server-side) and webpack(client-side)",
  "main": "index.js",
  "scripts": {
    "build:server": "NODE_ENV=production babel src --out-dir dist",
    "build:client": "NODE_ENV=production ubuild",
    "build": "run-p build:server build:client",
    "serve": "run-p serve:server serve:client",
    "lserve": "run-s ulink serve",
    "serve:server": "node ./src/server/index.js",
    "serve:client": "udev",
    "servep": "NODE_PATH=./ NODE_ENV=production node ./dist/server/index.js",
    "ulink:ur": "cd ../../packages/react-ur && npm run build && cp ./dist/index.js ../../example/simple/react-ur.js",
    "ulink:ur-tools": "npm link ../../packages/react-ur-tools",
    "clean:cache": "rm -rf ./node_modules/.cache",
    "ulink": "run-p clean:cache ulink:ur ulink:ur-tools"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-rollup.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-rollup/issues"
  },
  "_moduleAliases": {
    "@app/pages": "./dist/pages"
  },
  "_moduleDirectories": [
    "./"
  ],
  "homepage": "https://github.com/subuta/play-with-rollup#readme",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "loadable-components": "^2.2.3",
    "module-alias": "^2.1.0",
    "next": "^7.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.7",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/register": "^7.0.0",
    "clear-module": "^3.0.0",
    "esm": "^3.0.83",
    "npm-run-all": "^4.1.3",
    "sane": "^3.0.0"
  }
}
