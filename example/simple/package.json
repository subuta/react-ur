  "bugs": {
    "url": "https://github.com/subuta/play-with-rollup/issues"
  },
  "homepage": "https://github.com/subuta/play-with-rollup#readme",
  "dependencies": {
    "esm": "^3.0.84",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.5.3",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "loadable-components": "^2.2.3",
    "lodash": "^4.17.11",
    "promise-timeout": "^1.3.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.7",
    "react-router-dom": "^4.3.1",
    "react-ur": "^0.1.5",
    "react-ur-tools": "^0.1.6",
    "recompose": "^0.30.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "clear-module": "^3.0.0",
    "sane": "^3.0.0",
    "npm-run-all": "^4.1.3"
  }
}
