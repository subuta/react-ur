{
  "name": "play-with-rollup",
  "version": "1.0.0",
  "description": "Example setup for various Rollup module(es/umd/cjs) type with babel(server-side) and webpack(client-side)",
  "main": "index.js",
  "scripts": {
    "build:server": "babel src --out-dir dist",
    "build:client": "NODE_ENV=production webpack --silent",
    "build": "run-p build:server build:client",
    "serve": "run-p serve:server serve:client",
    "serve:server": "node ./src/server/index.js",
    "serve:client": "webpack-dev-server",
    "servep:server": "NODE_PATH=./ NODE_ENV=production node ./dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-rollup.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-rollup/issues"
  },
  "homepage": "https://github.com/subuta/play-with-rollup#readme",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.7",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "clear-module": "^3.0.0",
    "esm": "^3.0.83",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "sane": "^3.0.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
